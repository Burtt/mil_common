<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <param name="/is_simulation" value="True" />

  <node pkg="mil_usb_to_can" type="driver.py" name="usb_to_can_driver">
    <rosparam>
    device_handles:
      "8": mil_usb_to_can.ActuatorDeviceHandle
    simulated_devices:
      "8": mil_usb_to_can.ExampleSimulatedEchoDevice
    </rosparam>
  </node>

  <test test-name="actuator_test" pkg="mil_usb_to_can" type="actuator_device_test.py" />
</launch>
